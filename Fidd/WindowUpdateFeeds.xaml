<Window x:Class="Fidd.WindowUpdateFeeds"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fidd"
        mc:Ignorable="d"
        Title="Updating Feeds..." SizeToContent="Height" Width="400"
        WindowStartupLocation="CenterScreen" ResizeMode="NoResize"
        Loaded="Window_Loaded">
    <StackPanel Orientation="Vertical" Margin="8">
        <ProgressBar x:Name="ProgressFeedUpdate" Height="20"/>
        <TextBlock x:Name="TextStatus" Margin="0, 8, 0, 0"/>
    </StackPanel>
</Window>
